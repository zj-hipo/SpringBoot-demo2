<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zj.mapper.RegisterMapper">
    <insert id="insertCustomer">
        INSERT INTO customer(username,nickname,password,sex,age)VALUES (#{username},#{nickname},#{password},#{sex},#{age})
    </insert>
</mapper>